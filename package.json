{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_OPTIONS='-r tsconfig-paths/register' nodemon --watch server --watch lib --ext ts,json --exec 'tsx' server/index.ts",
    "dev:client": "cd client && vite",
    "build": "vite build && npm run build:server",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/server",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "generate:api": "openapi-generator-cli generate -i docs/planning/openapi.yaml -g typescript-fetch -o lib/api/generated --additional-properties=supportsES6=true,npmName=@helphut/api,withInterfaces=true",
    "generate:types": "openapi-generator-cli generate -i docs/planning/openapi.yaml -g typescript-axios -o lib/types/generated --additional-properties=supportsES6=true,npmName=@helphut/types",
    "generate": "npm run generate:api && npm run generate:types"
  },
